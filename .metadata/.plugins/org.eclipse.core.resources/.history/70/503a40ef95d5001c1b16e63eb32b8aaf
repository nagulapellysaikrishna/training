package com.main;

import java.util.Scanner;


public class Question8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String val = sc.next();
		String[ ] arr = val.split(val);
		System.out.println(arr);
		for (String t:arr){
			System.out.println(t);
		}
			


}
}
