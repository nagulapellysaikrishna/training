package com.service;

public class Question2Sevice {
	public int add(int first,int second) {
		return first+second;
	}
	public int sub(int first, int second) {
		return first-second;
	}
	public int mutlipy(int first, int second) {
		return first*second;
	}
	public int division(int first, int second) {
		return first/second;
	}
	public int remainder(int first, int second) {
		return first*second;
	}
}
