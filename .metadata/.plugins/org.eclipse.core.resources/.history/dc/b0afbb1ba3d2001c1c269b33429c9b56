package com.model;

public class MyinterfaceImp1{

	public int add(int a, int b) {
		return a+b;
	}

	public int sub(int a, int b) {
		return a-b;
	}

	public int multiply(int a, int b) {
		return a*b;
	}

	public int div(int a, int b) {
		return a/b;
	}

	public int remainder(int a, int b) {
		return a%b;
	}
	public double sqrt(int num) {
		if(num < 2) {
			return num;
		}
		
		int sqrtVal = 1;
		while(sqrtVal * sqrtVal <= num) {
			sqrtVal++;
		}
		return sqrtVal - 1;
	}
	
}
