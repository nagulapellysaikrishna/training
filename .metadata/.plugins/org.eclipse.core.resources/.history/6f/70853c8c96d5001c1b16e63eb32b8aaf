package com.main;

import java.util.Scanner;


public class Question8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String val = sc.next();
		char [] arr = val.toCharArray();
		System.out.println(arr);
		for (char t:arr){
			System.out.println(t);
}
			


}
}
